

.giveLists{
    font-size: 28rpx;
    color: rgba(241, 241, 241, 1);
}
.title{
    color: #91989F;
    padding: 24rpx 30rpx;
    line-height: 40rpx;
}
.contentList{
    padding: 10rpx 30rpx 30rpx 22rpx;
}
.item{
    display: flex;
    align-items: center;
    justify-content: space-between;
    border-bottom: 1rpx solid rgba(255,255,255,0.1);
    padding: 22rpx 0 22rpx 0;
}
.itemLeft{
    display: flex;
    align-items: center;
}
.iconBox{
    width: 50rpx;
    height: 50rpx;
    display: flex;
    align-items: center;
    justify-content: center;
}
.imgBox{
    width: 72rpx;
    height: 72rpx;
    display: flex;
    align-items: center;
    justify-content: center;
    margin-left: 60rpx;
    margin-right: 48rpx;
    overflow: hidden;
    border-radius: 72rpx;
    -webkit-border-radius: 72rpx;
    -moz-border-radius: 72rpx;
    -ms-border-radius: 72rpx;
    -o-border-radius: 72rpx;
}
image{
    border: 0;
    height: auto;
}
.nicname{
    font-size: 32rpx;
}
.giveNum{
    color: #91989F;
}