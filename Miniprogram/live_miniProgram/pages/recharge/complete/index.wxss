.complete-icon {
    margin-top: 40rpx;
    height: 320rpx;
}

.complete-successful {
    font-size: 40rpx;
    color: rgba(241, 241, 241, 1);
    font-weight: bold;
    text-align: center;
}

.complete-tip {
    margin-top: 40rpx;
    font-size: 28rpx;
    color: rgba(145, 152, 159, 1);
    text-align: center;
}

.complete-button {
    height: 94rpx;
    margin-left: 24rpx;
    margin-right: 24rpx;
    margin-top: 80rpx;
    background-color: rgba(23, 96, 255, 1);

    border-radius: 10rpx;

    font-size: 36rpx;
    color: rgba(241, 241, 241, 1);

    display: flex;
    flex-direction: row;
    justify-content: center;
    align-items: center;
    align-content: center;
}